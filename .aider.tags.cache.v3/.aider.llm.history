USER                 status: 200,
USER                 body: JSON.stringify(formStructure),
USER                 headers: {
USER                     'Content-Type': 'application/json'
USER                 }
USER             };
USER         } catch (error) {
USER             return {
USER                 status: 500,
USER                 body: JSON.stringify({ error: 'Failed to generate form' })
USER             };
USER         }
USER     }
USER });
USER ```

